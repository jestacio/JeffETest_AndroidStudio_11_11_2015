<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.jeffe.gtp"
android:versionCode="1"
android:versionName="1.0" >

<uses-sdk
    android:minSdkVersion="15"
    android:targetSdkVersion="24" />

<application
    android:allowBackup="true"
    android:icon="@drawable/ic_launcher" >
    <activity
        android:name=".MainActivity"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:label="@string/app_name" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />\
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name=".ListGamesActivity"
        android:label="@string/title_listgames"
        android:theme="@style/Theme.AppCompat" >
    </activity>
    <activity
        android:name=".RateGamesActivity"
        android:label="@string/title_rategames"
        android:theme="@style/Theme.AppCompat" >
    </activity>
    <activity
        android:name=".AddGameListingActivity"
        android:label="@string/title_addgame"
        android:theme="@style/Theme.AppCompat" >
    </activity>
</application>

</manifest>
